{
	"name": "dokuly-plm",
	"version": "1.0.0",
	"description": "https://github.com/Dokuly-PLM/dokuly",
	"main": "index.js",
	"directories": {
		"test": "tests"
	},
	"scripts": {
		"dev": "concurrently \"webpack --mode development --watch\" \"browser-sync start --proxy localhost:8000 --files 'dokuly/frontend/static/frontend/**/*' --no-open --reload-delay 500\"",
		"dev:old": "webpack --mode development --watch ./dokuly/frontend/src/index.js --output-path ./dokuly/frontend/static/frontend/",
		"dev:frontend": "webpack --mode development --watch",
		"dev:docker": "docker-compose -f docker-compose-dev.yml up --build",
		"dev:docker:mac": "docker-compose -f docker-compose-dev-mac.yml up --build",
		"build": "webpack --mode production",
		"build:watch": "webpack --mode development --watch",
		"lint": "eslint dokuly/frontend/src --ext .js,.jsx --fix",
		"lint:check": "eslint dokuly/frontend/src --ext .js,.jsx",
		"format": "prettier --write dokuly/frontend/src/**/*.{js,jsx,css,scss}",
		"format:check": "prettier --check dokuly/frontend/src/**/*.{js,jsx,css,scss}",
		"prepare": "husky install",
		"public_dev": "webpack --mode development --watch ./dokuly/public_page/public_page/src/index.js --output-path ./dokuly/public_page/static/",
		"public_build": "webpack --mode production ./dokuly/public_page/public_page/src/index.js --output-path ./dokuly/public_page/static/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Dokuly-PLM/dokuly"
	},
	"author": "DokulyOSS",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Dokuly-PLM/dokuly/issues"
	},
	"homepage": "https://github.com/Dokuly-PLM/dokuly#readme",
	"dependencies": {
		"@babel/core": "^7.20.12",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
